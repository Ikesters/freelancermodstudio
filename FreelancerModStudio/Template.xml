<?xml version="1.0" encoding="utf-8"?>
<FreelancerModStudio-Template-1.0 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Files>
    <File name="universe">
      <Path>universe\universe.ini</Path>
      <Blocks>
        <Block name="Time">
          <Options>
            <Option type="Int">seconds_per_day</Option>
          </Options>
        </Block>
        <Block name="Base" multiple="true" identifier="nickname">
          <Options>
            <Option>nickname</Option>
            <Option>system</Option>
            <Option type="Int">strid_name</Option>
            <Option>file</Option>
            <Option>BGCS_base_run_by</Option>
            <Option>terrain_tiny</Option>
            <Option>terrain_sml</Option>
            <Option>terrain_mdm</Option>
            <Option>terrain_lrg</Option>
            <Option>terrain_dyna_01</Option>
            <Option>terrain_dyna_02</Option>
            <Option type="Bool">autosave_forbidden</Option>
          </Options>
        </Block>
        <Block name="System" multiple="true" identifier="nickname">
          <Options>
            <Option>nickname</Option>
            <Option>file</Option>
            <Option type="Point">pos</Option>
            <Option>msg_id_prefix</Option>
            <Option type="Int">visit</Option>
            <Option type="Int">strid_name</Option>
            <Option type="Int">ids_info</Option>
            <Option type="Double">NavMapScale</Option>
          </Options>
        </Block>
      </Blocks>
    </File>
    <File name="system">
      <Path>universe\system\*\*.ini</Path>
      <Blocks>
        <Block name="SystemInfo">
          <Options>
            <Option>name</Option>
            <Option type="RGB">space_color</Option>
            <Option category="1">local_faction</Option>
          </Options>
        </Block>
        <Block name="Music">
          <Options>
            <Option>space</Option>
            <Option>danger</Option>
            <Option>battle</Option>
          </Options>
        </Block>
        <Block name="Archetype">
          <Options>
            <Option>ship</Option>
            <Option>simple</Option>
            <Option>snd</Option>
            <Option>solar</Option>
            <Option>equipment</Option>
          </Options>
        </Block>
        <Block name="Dust">
          <Options>
            <Option>spacedust</Option>
          </Options>
        </Block>
        <Block name="Ambient">
          <Options>
            <Option type="RGB">color</Option>
          </Options>
        </Block>
        <Block name="Texturepanels">
          <Options>
            <Option>file</Option>
          </Options>
        </Block>
        <Block name="Background">
          <Options>
            <Option>basic_stars</Option>
            <Option>complex_stars</Option>
            <Option>nebulae</Option>
          </Options>
        </Block>
        <Block name="Zone" multiple="true" identifier="nickname">
          <Options>
            <Option>nickname</Option>
            <Option>pos</Option>
            <Option>rotate</Option>
            <Option>shape</Option>
            <Option>size</Option>
            <Option>encounter</Option>
            <Option>faction</Option>
            <Option>faction_weight</Option>
            <Option>density_restriction</Option>
            <Option>usage</Option>
            <Option>path_label</Option>
            <Option>pop_type</Option>
            <Option>spacedust</Option>
            <Option>music</Option>
            <Option>vignette_type</Option>
            <Option>mission_type</Option>
            <Option>comment</Option>
            <Option>lane_id</Option>
            <Option>attack_ids</Option>
            <Option type="Bool">population_additive</Option>
            <Option type="Bool">mission_eligible</Option>
            <Option type="Int">ids_name</Option>
            <Option type="Int">ids_info</Option>
            <Option type="Int">toughness</Option>
            <Option type="Int">density</Option>
            <Option type="Int">repop_time</Option>
            <Option type="Int">max_battle_size</Option>
            <Option type="Int">relief_time</Option>
            <Option type="Int">visit</Option>
            <Option type="Int">tradelane_down</Option>
            <Option type="Int">spacedust_maxparticles</Option>
            <Option type="Int">property_flags</Option>
            <Option type="Int">tradelane_attack</Option>
            <Option type="Double">damage</Option>
            <Option type="Double">sort</Option>
            <Option type="Double">edge_fraction</Option>
            <Option type="Double">interference</Option>
            <Option type="DoubleArray">property_fog_color</Option>
          </Options>
        </Block>
        <Block name="Object" multiple="true" identifier="nickname">
          <Options>
            <Option>nickname</Option>
            <Option>pos</Option>
            <Option>rotate</Option>
            <Option>spin</Option>
            <Option>msg_id_prefix</Option>
            <Option>jump_effect</Option>
            <Option>reputation</Option>
            <Option>behavior</Option>
            <Option>star</Option>
            <Option>archetype</Option>
            <Option>loadout</Option>
            <Option>pilot</Option>
            <Option>base</Option>
            <Option>voice</Option>
            <Option>comment</Option>
            <Option>prev_ring</Option>
            <Option>next_ring</Option>
            <Option>dock_with</Option>
            <Option>parent</Option>
            <Option>faction</Option>
            <Option>ambient</Option>
            <Option type="Int">difficulty_level</Option>
            <Option type="Int">atmosphere_range</Option>
            <Option type="Int">ids_name</Option>
            <Option type="Int">ids_info</Option>
            <Option type="Int">visit</Option>
            <Option type="Int">tradelane_space_name</Option>
            <Option type="RGB">color</Option>
            <Option type="RGB">ambient_color</Option>
            <Option type="RGB">burn_color</Option>
            <Option type="StringArray">goto</Option>
            <Option type="StringArray">space_costume</Option>
          </Options>
        </Block>
        <Block name="LightSource" multiple="true" identifier="nickname">
          <Options>
            <Option>nickname</Option>
            <Option>pos</Option>
            <Option type="RGB">color</Option>
            <Option>range</Option>
            <Option>type</Option>
            <Option>atten_curve</Option>
            <Option>attenuation</Option>
          </Options>
        </Block>
        <Block name="Asteroids" multiple="true" identifier="zone">
          <Options>
            <Option>file</Option>
            <Option>zone</Option>
          </Options>
        </Block>
        <Block name="Nebula" multiple="true" identifier="file">
          <Options>
            <Option>file</Option>
            <Option>zone</Option>
          </Options>
        </Block>
        <Block name="EncounterParameters" multiple="true" identifier="nickname">
          <Options>
            <Option>nickname</Option>
            <Option>filename</Option>
          </Options>
        </Block>
      </Blocks>
    </File>
  </Files>
  <CostumTypes>
    <Enums>
      <Enum name="Direction">
        <Values>
          <Value>top</Value>
          <Value>bottom</Value>
          <Value>right</Value>
          <Value>left</Value>
        </Values>
      </Enum>
    </Enums>
  </CostumTypes>
  <Languages>
    <Language id="en">
      <Comments>
        <Comment file="Universe" block="System" option="nickname">the nickname of the system</Comment>
        <Comment file="Universe" block="System" option="file">the filepath of the system</Comment>
      </Comments>
      <Categories>
        <Category id="0">General</Category>
